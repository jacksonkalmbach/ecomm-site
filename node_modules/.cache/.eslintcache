ail } = userAuth;\n    const createdAt = new Date();\n    \n    // create/set the document with the data from the userAuth in collection\n    try {\n      await setDoc(userDocRef, {\n        displayName,\n        email,\n        createdAt, \n        ...additionalInfo,\n      });\n    } catch(error) {\n      console.log('error creating user', error.message);\n    }\n  }\n  // return userDocRef\n  return userDocRef;\n};\n\nexport const createAuthUserWithEmailAndPassword = async (email, password) => {\n  // if we are not given an email or a password, then exit the function\n  if (!email || !password) return;\n\n  // otherwise, return the value from calling the method\n  return await createUserWithEmailAndPassword(auth, email, password);\n};","/Users/jacksonkalmbach/Dropbox/Mac/Desktop/dev/projects/ecommerce-project/src/components/signUpForm/signUpForm.component.jsx",["67"],[],"/Users/jacksonkalmbach/Dropbox/Mac/Desktop/dev/projects/ecommerce-project/src/components/form-input/form-input.component.jsx",[],[],"/Users/jacksonkalmbach/Dropbox/Mac/Desktop/dev/projects/ecommerce-project/src/components/button/button.component.jsx",[],[],{"ruleId":"68","severity":1,"message":"69","line":9,"column":11,"nodeType":"70","messageId":"71","endLine":9,"endColumn":21},{"ruleId":"68","severity":1,"message":"72","line":27,"column":7,"nodeType":"70","messageId":"71","endLine":27,"endColumn":18},{"ruleId":"68","severity":1,"message":"69","line":40,"column":13,"nodeType":"70","messageId":"71","endLine":40,"endColumn":23},"no-unused-vars","'userDocRef' is assigned a value but never used.","Identifier","unusedVar","'firebaseApp' is assigned a value but never used."]